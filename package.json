{
	"name": "tsb-resource-pack-generator",
	"displayName": "TSB Resource pack Generator",
	"description": "",
	"version": "1.1.0",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/TheSkyBlock/resource-pack-generator.git"
	},
	"license": "MIT",
	"publisher": "ChenCMD",
	"author": "ChenCMD",
	"activationEvents": [
		"onCommand:tsb-resource-pack-generator.gen"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "tsb-resource-pack-generator.gen",
				"title": "Resource Pack Generator",
				"category": "ResourcePack"
			}
		],
		"configuration": {
			"title": "MC Resourcepack Generator",
			"properties": {
				"mcrg.customizeInjectFolder": {
					"type": "string",
					"pattern": "^[^\\\\:*?\"<>|]*$",
					"description": "生成するフォルダ"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"build": "webpack --mode development",
		"watch": "tsc -w"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"ts-loader": "^8.0.12",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0",
		"webpack": "^5.11.0",
		"webpack-cli": "^4.2.0"
	},
	"dependencies": {}
}
