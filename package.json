{
	"name": "mc-resourcepack-generator",
	"displayName": "MC Resourcepack Generator",
	"description": "",
	"version": "1.1.3",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/ChenCMD/MC-Resourcepack-Generator.git"
	},
	"license": "MIT",
	"publisher": "ChenCMD",
	"author": {
		"name": "ChenCMD",
		"url": "https://twitter.com/CMD__Cat",
		"email": "scnmed.g@gmail.com"
	},
	"activationEvents": [
		"onCommand:mc-resourcepack-generator.gen"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "mc-resourcepack-generator.gen",
				"title": "Resourcepack Generator",
				"category": "ResourcePack"
			}
		],
		"configuration": {
			"title": "MC Resourcepack Generator",
			"properties": {
				"mcrg.customizeInjectFolder": {
					"type": "string",
					"pattern": "^[^\\\\:*?\"<>|]*$",
					"description": "テクスチャやmodelファイルを配置するフォルダ"
				}
			}
		}
	},
	"scripts": {
		"compile": "webpack --mode development",
		"build": "webpack --mode production",
		"watch": "webpack --mode none --watch/",
		"lint": "eslint --fix src/**/*.ts",
		"release": "semantic-release"
	},
	"devDependencies": {
		"@semantic-release/changelog": "^5.0.1",
		"@semantic-release/git": "^9.0.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/sharp": "^0.27.1",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"semantic-release": "^17.3.2",
		"semantic-release-gitmoji": "^1.3.4",
		"semantic-release-vsce": "^3.0.1",
		"ts-loader": "^8.0.12",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0",
		"webpack": "^5.11.0",
		"webpack-cli": "^4.2.0"
	},
	"dependencies": {
		"sharp": "^0.27.0"
	}
}
